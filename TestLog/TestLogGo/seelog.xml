<seelog type="asynctimer" asyncinterval="5000000" minlevel="info" maxlevel="error">
    <exceptions>
        <exception filepattern="*" levels="trace,debug,info,warn,error"/>
    </exceptions>
    <outputs formatid="main">
        <filter levels="info,warn,error">
            <console/>
        </filter>
    </outputs>
    <formats>
        <format id="main" format="%Date(2006-01-02T15:04:05-0700) %Time %Level [%File:%Line] - %Msg%n"/>
    </formats>
</seelog>
